<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/shape_result_fragment_background_white_stroke_10"
    android:orientation="vertical"
    android:padding="17dp"
    tools:context=".ResultNutriBarFragment">

    <!-- TODO: Update blank fragment layout -->
    <TextView
        android:id="@+id/result_bar_chart_detail"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fontFamily="@font/wanted_sans_extra_bold"
        android:text="@string/result_nutri_bar_chart_text1"
        android:textColor="@color/gray_950"
        android:textSize="16sp" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="23dp"
        android:layout_marginTop="20dp"
        android:gravity="center"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/naName"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:fontFamily="@font/wanted_sans_bold"
            android:gravity="center|left"
            android:text="@string/nutri_na"
            android:textColor="@color/black" />

        <TextView
            android:id="@+id/naQuant"
            android:layout_width="wrap_content"
            android:layout_height="23dp"
            android:layout_marginStart="4dp"
            android:fontFamily="@font/wanted_sans_extra_bold"
            android:gravity="center|left"
            android:text="150mg"
            android:textColor="@color/blue_700" />

        <TextView
            android:id="@+id/naDV"
            android:layout_width="wrap_content"
            android:layout_height="23dp"
            android:layout_weight="1"
            android:gravity="right|center"
            android:text="권장 섭취량 00g"
            android:fontFamily="@font/wanted_sans_semi_bold"
            android:textSize="13sp"
            android:textColor="@color/gray_300" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <com.github.mikephil.charting.charts.HorizontalBarChart
            android:id="@+id/naBarchart"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_weight="1"
            android:gravity="center" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="23dp"
        android:layout_marginTop="20dp"
        android:gravity="center"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/carbName"
            android:layout_width="wrap_content"
            android:layout_height="23dp"
            android:fontFamily="@font/wanted_sans_bold"
            android:gravity="center|left"
            android:text="@string/nutri_carb"
            android:textColor="@color/black" />

        <TextView
            android:id="@+id/carbQuant"
            android:layout_width="wrap_content"
            android:layout_height="23dp"
            android:layout_marginStart="4dp"
            android:fontFamily="@font/wanted_sans_extra_bold"
            android:gravity="center|left"
            android:text="18.2g"
            android:textColor="@color/blue_700" />

        <TextView
            android:id="@+id/carbDV"
            android:layout_width="match_parent"
            android:layout_height="23dp"
            android:layout_weight="1"
            android:gravity="right|center"
            android:text="권장 섭취량 00g"
            android:fontFamily="@font/wanted_sans_semi_bold"
            android:textSize="13sp"
            android:textColor="@color/gray_300" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <com.github.mikephil.charting.charts.HorizontalBarChart
            android:id="@+id/carbBarchart"
            android:layout_width="match_parent"
            android:padding="0dp"
            android:layout_height="30dp"
            android:gravity="center"
            android:layout_weight="1" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="23dp"
        android:layout_marginTop="20dp"
        android:gravity="center"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/sugarName"
            android:layout_width="wrap_content"
            android:layout_height="23dp"
            android:fontFamily="@font/wanted_sans_bold"
            android:gravity="center|left"
            android:text="@string/nutri_sugar"
            android:textColor="@color/black" />

        <TextView
            android:id="@+id/sugarQuant"
            android:layout_width="wrap_content"
            android:layout_height="23dp"
            android:layout_marginStart="4dp"
            android:fontFamily="@font/wanted_sans_extra_bold"
            android:gravity="center|left"
            android:text="1.3g"
            android:textColor="@color/blue_700" />

        <TextView
            android:id="@+id/sugarDV"
            android:layout_width="match_parent"
            android:layout_height="23dp"
            android:layout_weight="1"
            android:gravity="right|center"
            android:text="권장 섭취량 00g"
            android:fontFamily="@font/wanted_sans_semi_bold"
            android:textSize="13sp"
            android:textColor="@color/gray_300" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <com.github.mikephil.charting.charts.HorizontalBarChart
            android:id="@+id/sugarBarchart"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:gravity="center"
            android:layout_weight="1" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="23dp"
        android:layout_marginTop="20dp"
        android:gravity="center"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/fatName"
            android:layout_width="wrap_content"
            android:layout_height="23dp"
            android:fontFamily="@font/wanted_sans_bold"
            android:gravity="center|left"
            android:text="@string/nutri_fat"
            android:textColor="@color/black" />

        <TextView
            android:id="@+id/fatQuant"
            android:layout_width="wrap_content"
            android:layout_height="23dp"
            android:layout_marginStart="4dp"
            android:fontFamily="@font/wanted_sans_extra_bold"
            android:gravity="center|left"
            android:text="52g"
            android:textColor="@color/blue_700" />

        <TextView
            android:id="@+id/fatDV"
            android:layout_width="match_parent"
            android:layout_height="23dp"
            android:layout_weight="1"
            android:gravity="right|center"
            android:text="권장 섭취량 00g"
            android:fontFamily="@font/wanted_sans_semi_bold"
            android:textSize="13sp"
            android:textColor="@color/gray_300" />

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <com.github.mikephil.charting.charts.HorizontalBarChart
            android:id="@+id/fatBarchart"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:gravity="center"
            android:layout_weight="1" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="23dp"
        android:layout_marginTop="20dp"
        android:gravity="center"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/satFatName"
            android:layout_width="wrap_content"
            android:layout_height="23dp"
            android:fontFamily="@font/wanted_sans_bold"
            android:gravity="center|left"
            android:text="@string/nutri_satFat"
            android:textColor="@color/black" />

        <TextView
            android:id="@+id/satFatQuant"
            android:layout_width="wrap_content"
            android:layout_height="23dp"
            android:layout_marginStart="4dp"
            android:fontFamily="@font/wanted_sans_extra_bold"
            android:gravity="center|left"
            android:text="52g"
            android:textColor="@color/blue_700" />

        <TextView
            android:id="@+id/satFatDV"
            android:layout_width="match_parent"
            android:layout_height="23dp"
            android:layout_weight="1"
            android:gravity="right|center"
            android:text="권장 섭취량 00g"
            android:fontFamily="@font/wanted_sans_semi_bold"
            android:textSize="13sp"
            android:textColor="@color/gray_300" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <com.github.mikephil.charting.charts.HorizontalBarChart
            android:id="@+id/satFatBarchart"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:gravity="center"
            android:layout_weight="1" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="23dp"
        android:layout_marginTop="20dp"
        android:gravity="center"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/choleName"
            android:layout_width="wrap_content"
            android:layout_height="23dp"
            android:fontFamily="@font/wanted_sans_bold"
            android:gravity="center|left"
            android:text="@string/nutri_chole"
            android:textColor="@color/black" />

        <TextView
            android:id="@+id/choleQuant"
            android:layout_width="wrap_content"
            android:layout_height="23dp"
            android:layout_marginStart="4dp"
            android:fontFamily="@font/wanted_sans_extra_bold"
            android:gravity="center|left"
            android:text="0g"
            android:textColor="@color/blue_700" />

        <TextView
            android:id="@+id/choleDV"
            android:layout_width="match_parent"
            android:layout_height="23dp"
            android:layout_weight="1"
            android:gravity="right|center"
            android:text="권장 섭취량 00g"
            android:fontFamily="@font/wanted_sans_semi_bold"
            android:textSize="13sp"
            android:textColor="@color/gray_300" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <com.github.mikephil.charting.charts.HorizontalBarChart
            android:id="@+id/choleBarchart"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:gravity="center"
            android:layout_weight="1" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="23dp"
        android:layout_marginTop="20dp"
        android:gravity="center"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/proteinName"
            android:layout_width="wrap_content"
            android:layout_height="23dp"
            android:fontFamily="@font/wanted_sans_bold"
            android:gravity="center|left"
            android:text="@string/nutri_protein"
            android:textColor="@color/black" />

        <TextView
            android:id="@+id/proteinQuant"
            android:layout_width="wrap_content"
            android:layout_height="23dp"
            android:layout_marginStart="4dp"
            android:fontFamily="@font/wanted_sans_extra_bold"
            android:gravity="center|left"
            android:text="0g"
            android:textColor="@color/blue_700" />

        <TextView
            android:id="@+id/proteinDV"
            android:layout_width="match_parent"
            android:layout_height="23dp"
            android:layout_weight="1"
            android:gravity="right|center"
            android:text="권장 섭취량 00g"
            android:fontFamily="@font/wanted_sans_semi_bold"
            android:textSize="13sp"
            android:textColor="@color/gray_300" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <com.github.mikephil.charting.charts.HorizontalBarChart
            android:id="@+id/proteinBarchart"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:gravity="center"
            android:layout_weight="1" />
    </LinearLayout>


</LinearLayout>