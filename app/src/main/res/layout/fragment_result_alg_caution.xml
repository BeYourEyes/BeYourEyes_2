<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:importantForAccessibility="yes"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingVertical="14dp"
    android:paddingHorizontal="22dp"
    android:background="@drawable/shape_result_fragment_background_white_stroke_10">


    <ImageView
        android:id="@+id/imageView3"
        android:importantForAccessibility="no"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_gravity="center"
        android:layout_marginEnd="16dp"
        android:src="@drawable/ic_result_allergy_warning_24" />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:orientation="vertical">

        <TextView
            android:id="@+id/algCautionText1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/alg_caution_text1"
            android:textColor="@color/gray_950"
            android:fontFamily="@font/wanted_sans_bold"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/algCautionText2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/alg_caution_text2"
            android:textColor="@color/gray_950"
            android:fontFamily="@font/wanted_sans_bold"
            android:textSize="18sp"
            android:textStyle="bold" />
    </LinearLayout>

</LinearLayout>